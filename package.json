{
  "name": "vc-theme-b2b-vue",
  "version": "1.48.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:expose": "yarn dev --host",
    "build": "yarn validate && yarn generate:routes && vite build",
    "build:dev": "yarn build --mode=development",
    "build:watch": "yarn build:dev --watch",
    "compress": "vite-node --mode production scripts/compress-theme.ts --",
    "validate:types": "vue-tsc --build --force",
    "validate:dependencies": "depcruise client-app",
    "validate": "yarn validate:types && yarn validate:dependencies",
    "commitlint": "commitlint --edit",
    "lint-staged": "lint-staged",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .eslintignore",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --check client-app/",
    "prettier:fix": "yarn prettier --write",
    "test:unit": "vitest",
    "test:typing": "vitest --typecheck",
    "generate:certificates": "vite-node --mode production scripts/generate-certificates",
    "generate:graphql:schema": "graphql-codegen --config graphql-codegen.schema.config.ts --require ./scripts/graphql-codegen.js",
    "generate:graphql:types": "graphql-codegen --config graphql-codegen.types.config.ts --require ./scripts/graphql-codegen.js",
    "generate:graphql": "yarn generate:graphql:schema && yarn generate:graphql:types",
    "generate:routes": "vite-node --mode production scripts/generate-routes.ts",
    "generate:dependency-graph": "depcruise client-app --output-type dot | dot -T svg | depcruise-wrap-stream-in-html > artifacts/dependency-graph.html",
    "storybook:dev": "yarn generate:certificates && storybook dev --docs --port 6006 --no-open --https --ssl-cert ./.certificates/public.pem --ssl-key ./.certificates/private.pem",
    "storybook:build": "storybook build --docs",
    "storybook:compress": "vite-node --mode production scripts/compress-storybook.ts --",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.8.9",
    "@builder.io/sdk-vue": "^0.3.1",
    "@headlessui/vue": "^1.7.17",
    "@popperjs/core": "^2.11.8",
    "@unhead/vue": "^1.8.10",
    "@vee-validate/rules": "^4.12.2",
    "@vee-validate/yup": "^4.12.2",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.7.2",
    "dompurify": "^3.0.8",
    "flag-icons": "^7.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "^11.1.1",
    "maska": "^2.1.11",
    "swiper": "11.0.5",
    "tw-elements": "^1.1.0",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.11",
    "vue-html-secure": "^1.0.10",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@graphql-codegen/add": "5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@rollup/plugin-graphql": "^2.0.4",
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-a11y": "^7.4.6",
    "@storybook/addon-actions": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/cli": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.4.6",
    "@storybook/vue3-vite": "7.4.6",
    "@tsconfig/node20": "^20.1.2",
    "@types/adm-zip": "^0.5.5",
    "@types/dompurify": "^3.0.5",
    "@types/gtag.js": "^0.0.18",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "adm-zip": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "chalk": "^5.3.0",
    "dependency-cruiser": "^15.5.0",
    "eslint": "^8.49.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-sort-export-all": "^1.4.1",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "storybook": "7.4.6",
    "storybook-vue3-router": "^4.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-node": "^1.0.4",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-mkcert": "^1.17.3",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.11.0 <21.0.0"
  },
  "packageManager": "yarn@4.0.2"
}
